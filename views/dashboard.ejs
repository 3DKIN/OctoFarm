<% if(typeof printerCount != 'undefined' && printerCount > 0 ) {%>

<div class="sidenav text-white bg-dark mt-5 pt-2">
  <div class="row pl-2">
    <div class="col-md-12 pr-0">
      <button
              type="button"
              class="btn btn-secondary float-left d-block"
              href="#"
              aria-expanded="false"
              disabled
      >
        <i class="fas fa-print"></i> Printers
      </button>
      <button
              type="button"
              class="btn btn-secondary float-right d-block"
              href="#"
              aria-expanded="false"
              disabled
      >
        <%= printerCount %>
      </button>

      <button
              id="searchOfflineBtn"
              type="button"
              class="btn btn-success float-right d-block mr-1"
              href="#"
              aria-expanded="false"
      >
        <i class="fas fa-redo"></i>
      </button>
    </div>

  </div>
    <div class="row pl-2">
        <div class="cell col-md-12 pr-0">
      <button data-toggle="modal" data-target="#connectionModal" id="connectAllBtn" type="button" class="btn btn-success btn-sm float-left"><i class="fas fa-link"></i> Connect All</button>
      <button data-toggle="modal" data-target="#connectionModal" id="disconnectAllBtn" type="button" class="btn btn-danger btn-sm float-right"><i class="fas fa-unlink"></i> Disconnect All</button>
        </div>
    </div>

      <div class="pl-2" id="printerList">  <%- include('./partials/Printers/dashboardList.ejs') %></div>






</div>

<!-- Page content -->
<div class="main">
  <div class="row">
    <%- include('./partials/currentOperations.ejs') %>
  </div>
  <div class="row">
    <div
            id="dashboardInfo"
            class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-4"
    >
