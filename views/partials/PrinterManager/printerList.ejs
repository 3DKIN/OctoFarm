<% sortedIndex.forEach(sorted => { %>
    <tr id="printerCard-<%= printers[sorted.actualIndex]._id %>">
        <td id="printerSortIndex-<%= printers[sorted.actualIndex]._id %>">
            <%= printers[sorted.actualIndex].sortIndex %>
        </td>
        <td>
            <center>
            <form title="Select your printer for multi-printer commands" class="was-validated form-check-inline form-check">
                <div title="Select your printer for multi-printer commands" class="tag custom-control custom-checkbox mb-2 pr-2">
                    <input type="checkbox" class="custom-control-input" id="dashboardSelect-<%= printers[sorted.actualIndex]._id %>" required>
                    <label class="custom-control-label" for="dashboardSelect-<%= printers[sorted.actualIndex]._id %>" contenteditable="false"></label>
                </div>
            </form>
            </center>
        </td>
        <td><div id="printerName-<%= printers[sorted.actualIndex]._id %>" contenteditable="false"><%if(typeof printers[sorted.actualIndex].settingsApperance != "undefined" ){%> <%= printers[sorted.actualIndex].settingsApperance.name %> <%}%></div></td>
        <td>
            <button  title="Control Your Printer"
                     id="printerButton-<%= printers[sorted.actualIndex]._id %>"
                     type="button"
                     class="tag btn btn-primary btn-sm"
                     data-toggle="modal"
                     data-target="#printerManagerModal" disabled
            ><i class="fas fa-print"></i>
            </button>
            <button  title="Change your Printer Settings"
                    id="printerSettings-<%= printers[sorted.actualIndex]._id %>"
                                 type="button"
                                 class="tag btn btn-secondary btn-sm  d-none"
                                 data-toggle="modal"
                                 data-target="#printerSettingsModal" disabled
            ><i class="fas fa-cog"></i>
            </button>
            <a title="Open your Printers Web Interface"
               id="printerWeb-<%= printers[sorted.actualIndex]._id %>"
               type="button"
               class="tag btn btn-info btn-sm"
               target="_blank"
               href="<%= printers[sorted.actualIndex].printerURL %>" role="button"><i class="fas fa-globe-europe"></i></a>
            <button  title="Re-Sync your printer"
                    id="printerSyncButton-<%= printers[sorted.actualIndex]._id %>"
                     type="button"
                     class="tag btn btn-success btn-sm"
            >
                <i class="fas fa-sync"></i>
            </button>
            <div class="btn-group d-none">
                <button title="Toggle your printers power"
                        id="printerPower-<%= printers[sorted.actualIndex]._id %>"
                        class="btn btn-danger btn-sm" type="button" disabled>
                    <i class="textActive fas fa-power-off"></i>
                </button>
                <button title="Other power actions" type="button" class="btn btn-sm btn-danger dropdown-toggle dropdown-toggle-split" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span class="sr-only">Toggle Dropdown</span>
                </button>
                <div class="dropdown-menu text-center">
                    <a id="printerPowerOff-<%= printers[sorted.actualIndex]._id %>" title="Turn off your printer" class="dropdown-item d-none" href="#"><i class="textActive fas fa-power-off"></i> Power Off Printer</a>
                    <a id="printerPowerOn-<%= printers[sorted.actualIndex]._id %>" title="Turn on your printer" class="dropdown-item d-none" href="#"><i class="textComplete fas fa-power-off"></i> Power On Printer</a>
                    <div id="printerDropDownMaker-<%= printers[sorted.actualIndex]._id %>" class="dropdown-divider d-none"></div>
                    <a title="Restart OctoPrint Service" class="dropdown-item" href="#"><i class="textActive fas fa-redo"></i> Restart OctoPrint</a>
                    <a title="Reboot OctoPrint Host" class="dropdown-item" href="#"><i class="textActive fas fa-sync-alt"></i> Reboot Host</a>
                    <a title="Shutdown OctoPrint Host" class="dropdown-item" href="#"><i class="textActive fas fa-power-off"></i> Shutdown Host</a>
                </div>
            </div>
            <span  title="Drag and Change your Printers sorting"
                   id="printerSortButton-<%= printers[sorted.actualIndex]._id %>"
                   class="tag btn btn-light btn-sm sortableList"
            >
    <i class="fas fa-grip-vertical"></i>
    </span></td>
        <td><small><span title="<%= printers[sorted.actualIndex].hostDescription %>" id="hostBadge-<%= printers[sorted.actualIndex]._id %>" class="tag badge badge-<%= printers[sorted.actualIndex].hostStateColour.name %> badge-pill">
                <%= printers[sorted.actualIndex].hostState %></span></small></td>
        <td><small><span title="<%= printers[sorted.actualIndex].stateDescription %>" id="printerBadge-<%= printers[sorted.actualIndex]._id %>" class="tag badge badge-<%= printers[sorted.actualIndex].stateColour.name %> badge-pill">
                <%= printers[sorted.actualIndex].state %></span></small></td>
        <td><small><span title="<%= printers[sorted.actualIndex].webSocketDescription %>" id="webSocketIcon-<%= printers[sorted.actualIndex]._id %>" class="tag badge badge-<%= printers[sorted.actualIndex].webSocket %> badge-pill">
                <i  class="fas fa-plug"></i></span></small></td>
        <td><div id="printerGroup-<%= printers[sorted.actualIndex]._id %>" contenteditable="false"><%= printers[sorted.actualIndex].group %></div></td>
        <td><div id="printerURL-<%= printers[sorted.actualIndex]._id %>" contenteditable="false"><%= printers[sorted.actualIndex].printerURL %></div></td>
        <td><div id="printerCamURL-<%= printers[sorted.actualIndex]._id %>" contenteditable="false"><%= printers[sorted.actualIndex].camURL %></div></td>
        <td><div id="printerApiKey-<%= printers[sorted.actualIndex]._id %>" contenteditable="false"><%= printers[sorted.actualIndex].apikey %></div></td>
        <th id="printerOctoPrintVersion-<%= printers[sorted.actualIndex]._id %>"><%if(typeof printers[sorted.actualIndex].octoPrintVersion != "undefined" ){%> <%= printers[sorted.actualIndex].octoPrintVersion %> <%}%></td>
        <td><button id="deleteButton-<%= printers[sorted.actualIndex]._id %>" type="button" class="btn btn-danger btn-sm d-none">
                <i class="fas fa-trash"></i>
            </button></td>
    </tr>
 <% }) %>
