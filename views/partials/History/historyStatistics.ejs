<div class="row">
  <div class="col-lg-12 text-center text-light">
    <div class="row">
      <div class="col-lg-2">
        <b class="mt-1 pb-1"><i class="fas fa-print"></i> Status</b>
        <p class="mb-0 pt-3">
          <i class="fas fa-square text-success"></i>
          <b>Completed: <%= printStatistics.completed %></b>
          <i class="fas fa-square text-warning"></i>
          <b>Cancelled: <%= printStatistics.cancelled %></b>
          <i class="fas fa-square text-danger"></i>
          <b>Failed: <%= printStatistics.failed %></b>
          <br />
        </p>
        <div class="progress mb-0">
          <div
            class="progress-bar progress-bar-striped bg-success"
            role="progressbar"
            style="width: <%= printStatistics.completedPercent  %>% "
            aria-valuenow=" <%= printStatistics.completedPercent %>%"
            aria-valuemin="0"
            aria-valuemax="100"
          >
            <%= Math.round(printStatistics.completedPercent * 10 ) /10 %>%
          </div>
          <div
            class="progress-bar progress-bar-striped bg-warning"
            role="progressbar"
            style="width: <%= printStatistics.cancelledPercent %>% "
            aria-valuenow="<%= printStatistics.cancelledPercent %>%"
            aria-valuemin="0"
            aria-valuemax="100"
          >
            <%= Math.round(printStatistics.cancelledPercent * 10 ) /10 %>%
          </div>
          <div
          class="progress-bar progress-bar-striped bg-danger"
          role="progressbar"
          style="width: <%= printStatistics.failedPercent %>% "
          aria-valuenow="<%= printStatistics.failedPercent %>%"
          aria-valuemin="0"
          aria-valuemax="100"
        >
          <%= Math.round(printStatistics.failedPercent * 10 ) /10 %>%
        </div>
        </div>
      </div>
      <div class="col-lg-3">
        <b class="mt-1"><i class="fas fa-vector-square"></i> Prints</b>
        <div class="row">
          <div class="col-lg-6">
            <b><i class="fas fa-clock"></i> Longest Print Time</b> <br />
            <%= helpers.generateTime(printStatistics.longestPrint) %><br />
            <b><i class="fas fa-clock"></i> Shortest Print Time</b> <br />
            <%= helpers.generateTime(printStatistics.shortestPrint) %> <br />
          </div>
          <div class="col-lg-6">
            <b><i class="fas fa-clock"></i> Average Print Time <br /></b><%=
            helpers.generateTime(printStatistics.averagePrintTime) %><br />
            <b><i class="fas fa-star"></i> Most Printed File <br /></b><%=
            printStatistics.mostPrintedFile.replace(/_/g," ") %><br />
          </div>
        </div>
      </div>
      <div class="col-lg-2">
        <b class="mt-1"><i class="fas fa-print"></i> Printers</b>
        <div class="row">
          <div class="col-lg-12">
            <b><i class="fas fa-star"></i> Most Occurances</b> <br />
            <%= printStatistics.mostUsedPrinters %><br />
            <b><i class="fas fa-star-half-alt"></i> Least Occurances</b> <br />
            <%= printStatistics.leastUsedPrinters %> <br />
          </div>

        </div>
      </div>
      <div class="col-lg-3">
        <b class="mt-1"><i class="fas fa-toilet-paper"></i> Filament</b>
        <div class="row">
          <div class="col-lg-6">
            <b><i class="fas fa-clock"></i> Total Filament Usage</b> <br />
            <%= printStatistics.totalFilamentUsage.toFixed(2) %>g<br />
            <b><i class="fas fa-clock"></i> Average Filament Usage</b> <br />
            <%= printStatistics.averageFilamentUsage.toFixed(2) %>g <br />
          </div>
          <div class="col-lg-6">
            <b><i class="fas fa-ruler"></i> Highest Filament Usage <br /></b><%=
            printStatistics.highestFilamentUsage.toFixed(2) %>g<br />
            <b><i class="fas fa-clock"></i> Lowest Filament Usage <br /></b><%=
            printStatistics.lowestFilamentUsage.toFixed(2) %>g<br />

          </div>
        </div>
      </div>
      <div class="col-lg-2">
        <b class="mt-1"><i class="fas fa-tag"></i> Cost</b>
        <div class="row">
          <div class="col-lg-12">
            <b><i class="fas fa-clock"></i> Total Cost</b> <br />
            <%= printStatistics.totalCost.toFixed(2) %> <br />
            <b><i class="fas fa-clock"></i> Highest Cost</b> <br />
            <%= printStatistics.highestCost.toFixed(2) %><br />

          </div>
        </div>
      </div>
    </div>
  </div>
</div>
