<% if(typeof printerCount != 'undefined' && printerCount > 0 ) {%>
  <div id="listView" class="row">
    <% if(clientSettings.panelView.currentOp){%> <%-
    include('./partials/currentOperations.ejs') %> <% }%>
    <div class="col-lg-12 table-responsive-sm table-responsive-md">
      <table class="table table-dark text-center">
        <thead>
        <tr>
          <th class="sticky-table table-dark" scope="col">Name</th>
          <th class="sticky-table table-dark" scope="col">Status</th>
          <th class="sticky-table table-dark" scope="col" width="400px">
            Actions
          </th>
          <th class="sticky-table table-dark" scope="col" width="160px">
            Control
          </th>
          <th class="sticky-table table-dark" scope="col" width="700px">Current File</th>
          <th class="sticky-table table-dark printerTimeElapsed" scope="col" width="300px">
            Print Time Elapsed
          </th>
          <th class="sticky-table table-dark printerTimeElapsed" scope="col">
            Progress
          </th>
          <th class="sticky-table table-dark printerRemainingTime" scope="col" width="300px">
            Remaining Print Time
          </th>
          <th class="sticky-table table-dark" scope="col" width="750px">
            Tools
          </th>
          <th class="sticky-table table-dark" scope="col" width="250px">Bed / Chamber</th>
        </tr>
        </thead>
        <tbody id="printerArea">

        </tbody>
      </table>
    </div>
  </div>

<% }else{ %>
  <%- include('./addPrinters.ejs') %>
<% } %>
<script src="/assets/js/listViewRunner.min.js" type="module"></script>