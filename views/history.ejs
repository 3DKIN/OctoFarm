<% if(typeof history.length != 'undefined' && history.length > 0 ) {%>

  <% let historyTotals = helpers.historyTotals(history); %>
<div class="row">
  <div id="dashboardInfo" class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 mb-2">
    <div class="card text-white bg-dark h-100">
      <div class="card-header dashHeader p-0">
        <button
                type="button"
                class="btn btn-secondary d-block ml-1 mr-1 p-1"
                href="#"
                aria-expanded="false"
                disabled
        >
          <i class="fas fa-chart-bar"></i> History Statistics
        </button>


      </div>
      <div class="card-body pt-2 pb-0 text-center">
        <%- include('./partials/History/historyStatistics.ejs') %>

      </div>
    </div>
  </div>
</div>
<div class="row">
  <div id="dashboardInfo" class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 mb-2">
    <div class="card text-white bg-dark h-100">
      <div class="card-header dashHeader p-0">
        <button
                type="button"
                class="btn btn-secondary d-block ml-1 mr-1 p-1"
                href="#"
                aria-expanded="false"
                disabled
        >
          <i class="fas fa-chart-bar"></i> History Filters
        </button>


      </div>
      <div class="card-body pt-2 pb-0 text-center">
        <div class="row d-none" id="historyToolbar">
          <div class="col-12">
            <nav
                    data-jplist-control="pagination"
                    data-group="history"
                    data-items-per-page="25"
                    data-current-page="0"
                    data-disabled-class="disabled"
                    data-selected-class="active"
                    data-name="pagination"
                    data-id="history-pagination">

              <!-- first and previous buttons -->
              <div class="btn-group pagination d-inline-flex" role="group" aria-label="Basic example">
                <button type="button" class="btn btn-secondary" data-type="first">First</button>
                <button type="button" class="btn btn-secondary" data-type="prev">Previous</button>
              </div>


              <div class="btn-group pagination d-inline-flex" data-type="pages">
                <button type="button" class="btn btn-secondary" data-type="page">{pageNumber}</button>

              </div>

              <div class="btn-group pagination d-inline-flex" role="group" aria-label="Basic example">
                <button type="button" class="btn btn-secondary" data-type="next">Next</button>
                <button type="button" class="btn btn-secondary" data-type="lase">Last</button>
              </div>

              <!-- items per page dropdown -->
              <div class="input-group mb-3 d-inline-flex w-25">
                <div class="input-group-prepend">
                  <label class="input-group-text" for="inputGroupSelect01">Items per page: </label>
                </div>
                <select data-type="items-per-page" class="custom-select" id="inputGroupSelect01">
                  <option value="25">25</option>
                  <option value="50">50</option>
                  <option value="75">75</option>
                  <option value="100"> 100 </option>
                  <option value="0"> All </option>
                </select>
              </div>

              <!-- information labels -->
              <span data-type="info" class="badge badge-secondary ml-3 p-2">
                    Page {pageNumber} of {pagesNumber}
              </span>

            </nav>
          </div>
          <div class="col-12 col-md-6 col-lg-3">
            <div class="input-group mb-3">
              <div class="input-group-prepend">
                <label class="input-group-text" for="inputGroupSelect01">File Name: </label>
              </div>
              <select class="custom-select" id="inputGroupSelect01"
                      data-jplist-control="select-filter"
                      data-group="history"
                      data-name="fileNames"
              >
                <option selected
                        href="#"
                        data-value="0"
                        data-path="default"
                >Filter</option>
                <% historyTotals.fileNames.forEach(file => { %>
                  <option href="#"
                          data-path=".<%= file %>"><%= file.replace(/_/g, " ") %></option>
                <%  }) %>

              </select>
            </div>
          </div>
          <div class="col-12 col-md-6 col-lg-3">
            <div class="input-group mb-3">
              <div class="input-group-prepend">
                <label class="input-group-text" for="inputGroupSelect01">Printer Name: </label>
              </div>
              <select class="custom-select" id="inputGroupSelect01"
                      data-jplist-control="select-filter"
                      data-group="history"
                      data-name="printerNames"
              >
                <option selected
                        href="#"
                        data-value="0"
                        data-path="default"
                >Filter</option>
                <% historyTotals.printerNames.forEach(printer => { %>
                  <option href="#"
                          data-path=".<%= printer %>"><%= printer.replace(/_/g, " ") %></option>
                <%  }) %>

              </select>
            </div>
          </div>
          <div class="col-12 col-md-6 col-lg-3">
            <div class="input-group mb-3">
              <div class="input-group-prepend">
                <label class="input-group-text" for="inputGroupSelect01">Filament Type: </label>
              </div>
              <select class="custom-select" id="inputGroupSelect01"
                      data-jplist-control="select-filter"
                      data-group="history"
                      data-name="spoolName"
              >
                <option selected
                        href="#"
                        data-value="0"
                        data-path="default"
                >Filter</option>
                <% historyTotals.spools.forEach(spool => { %>
                  <option href="#"
                          data-path=".<%= spool %>"><%= spool %></option>
                <%  }) %>

              </select>
            </div>
          </div>
          <div class="col-12 col-md-6 col-lg-3">

            <div class="dropdown mr-3"
                 data-jplist-control="dropdown-sort"
                 data-opened-class="show"
                 data-group="history"
                 data-name="default-sort"
                 data-id="default-sort">

              <button
                      data-type="panel"
                      class="btn btn-primary dropdown-toggle"
                      type="button">
                Default Sort
              </button>

              <div
                      data-type="content"
                      class="dropdown-menu"
                      aria-labelledby="dropdownMenuButton">

                <a class="dropdown-item"
                   href="#"
                   data-path="default">Default Sort</a>

                <a class="dropdown-item"

                   data-path=".index"
                   data-order="asc"
                   value="1"
                   data-type="number"><i class="fas fa-sort-numeric-down"></i> Index</a>

                <a class="dropdown-item"
                   href="#"
                   data-path=".index"
                   data-order="desc"
                   value="2"
                   data-type="number"><i class="fas fa-sort-numeric-up"></i> Index</a>

                <a class="dropdown-item"
                   href="#"
                   data-path=".printer"
                   data-order="asc"
                   value="3"
                   data-type="text"><i class="fas fa-sort-alpha-down"></i> Printer Name</a>

                <a class="dropdown-item"
                   href="#"
                   data-path=".printer"
                   data-order="desc"
                   value="4"
                   data-type="text"><i class="fas fa-sort-alpha-up"></i> Printer Name</a>
                <a class="dropdown-item"
                   href="#"
                   data-path=".file"
                   data-order="asc"
                   value="5"
                   data-type="text"><i class="fas fa-sort-alpha-down"></i> File Name</a>
                <a class="dropdown-item"
                   href="#"
                   data-path=".file"
                   data-order="desc"
                   value="6"
                   data-type="text"><i class="fas fa-sort-alpha-up"></i> File Name</a>

                <a class="dropdown-item"
                   href="#"
                   data-path=".start"
                   data-order="asc"
                   value="7"
                   data-type="datetime"><i class="fas fa-sort-numeric-down"></i> Start Date</a>
                <a class="dropdown-item"
                   href="#"
                   data-path=".start"
                   data-order="desc"
                   value="8"
                   data-type="datetime"><i class="fas fa-sort-numeric-up"></i> Start Date</a>

                <a class="dropdown-item"
                   href="#"
                   data-path=".end"
                   data-order="asc"
                   value="9"
                   data-type="datetime"><i class="fas fa-sort-numeric-down"></i> End Date</a>
                <a class="dropdown-item"
                   href="#"
                   data-path=".end"
                   data-order="desc"
                   value="10"
                   data-type="datetime"><i class="fas fa-sort-numeric-up"></i> End Date</a>

                <a class="dropdown-item"
                   href="#"
                   data-path=".cost"
                   data-order="asc"
                   value="11"
                   data-type="numnber"><i class="fas fa-sort-numeric-down"></i> Cost</a>
                <a class="dropdown-item"
                   href="#"
                   data-path=".cost"
                   data-order="desc"
                   value="12"
                   data-type="number"><i class="fas fa-sort-numeric-up"></i> Cost</a>


              </div>
            </div>


          </div>
          <div class="col-12">
<!--            <div class="row">-->
<!--              <div class="col-6">-->
<!--                Total Cost:-->
<!--              </div>-->
<!--              <div class="col-6">-->
<!--                Total Usage:-->
<!--              </div>-->
<!--            </div>-->
          </div>
          <!-- pagination control -->

        </div>
      <div class="row">
        <div class="col-12"><p id="loading">Loading...</p></div>
      </div>
      </div>
    </div>
  </div>
</div>

<div class="row">
  <div id="wrapper" class="col-lg-12 table-responsive-sm table-responsive-md d-none">
    <table id="historyTableTop" class="table table-dark text-center">
      <thead>
        <tr>
          <th class="table-dark" scope="col">#</th>
          <th class="table-dark" scope="col">State</th>
          <th class="table-dark" scope="col">Printer</th>
          <th class="table-dark" scope="col">File Name</th>
          <th class="table-dark" scope="col">Start Date</th>
          <th class="table-dark" scope="col">End Date</th>
          <th class="table-dark" scope="col">Print Time</th>
          <th class="table-dark" scope="col">Cost</th>
          <th class="table-dark" scope="col">Spool</th>
          <th class="table-dark" scope="col">Filament Usage</th>
          <th class="table-dark" scope="col" width="400px">
            Notes
          </th>
          <th class="sticky-table table-dark" scope="col"></th>
        </tr>
      </thead>
      <tbody id="historyTable" data-jplist-group="history">
        <% for(let i = 0; i < history.length; i++){ %>
        <tr data-jplist-item>
          <td class="index" scope="row">
            <%= history[i].printHistory.historyIndex %>
          </td>
          <td class="state">
            <% if(history[i].printHistory.success){%>
            <p class="d-none">Success</p><i class="fas fa-thumbs-up text-success"></i>
            <% }else{ %> <% if(history[i].printHistory.reason === "cancelled"){ %>
                <p class="d-none">Cancelled</p><i class="fas fa-thumbs-down text-warning"></i>
            <% }else{ %>
                <p class="d-none">Failure</p><i class="fas fa-exclamation text-danger"></i>
            <% } %> <% } %>
          </td>
          <td class="printer <%= history[i].printHistory.printerName.replace(/ /g, "_") %>"><%=
            history[i].printHistory.printerName %>
          </td>
          <td class="file <%= history[i].printHistory.fileName.replace(".gcode", "") %>" class="text-wrap">
            <%= history[i].printHistory.fileName.replace(/_/g," ") %>
          </td>
          <td class="start">
            <%= history[i].printHistory.startDate %>
          </td>
          <td class="end">
            <%= history[i].printHistory.endDate %>
          </td>
          <td class="time">
            <%= helpers.generateTime(history[i].printHistory.printTime) %>
          </td>
          <td class="cost" id="cost-<%= history[i]._id %>">
            Loading...
          </td>
          <%if(history[i].printHistory.filamentSelection != null){%>
          <td class="spool <%= history[i].printHistory.filamentSelection.spools.profile.material %>" id="spool-<%= history[i]._id %>">
          <% }else{%>
          <td class="spool" id="spool-<%= history[i]._id %>">
          <%}%>

            Loading...
          </td>
          <td class="usage" id="usage-<%= history[i]._id %>">
            Loading...
          </td>
          <td class="notes" class="text-wrap" id="note-<%= history[i]._id %>">
            <%= history[i].printHistory.notes %>
          </td>
          <td>
            <div class="btn-group" role="group" aria-label="Basic example">
              <button
                type="button"
                class="btn btn-info btn-small historyEdit"
                id="<%= history[i]._id %>"
                data-toggle="modal"
                data-target="#historyModal"
              >
                <i class="fas fa-eye"></i>
              </button>

              <button
                type="button"
                class="btn btn-danger btn-small historyDelete"
                id="<%= history[i]._id %>"
              >
                <i class="fas fa-trash"></i>
              </button>
            </div>
          </td>
        </tr>
        <% } %>
      </tbody>

    </table>

  </div>
</div>

<% }else{ %>
  <div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body text-center">
        <h1>No History yet</h1>
        <p>
          Come back here when you've completed some prints.
        </p>
        <h1><img width="500px" src="images/logo.png" /></h1>
      </div>
    </div>
  </div>
<% } %>
<div class="modal fade" id="historyModal" tabindex="0" role="dialog" aria-labelledby="historyModal" aria-hidden="true">
  <div class="modal-dialog modal-xl" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h3 class="modal-title" id="historyModalLabel"><button type="button" class="btn btn-secondary float-right d-block" href="#" aria-expanded="false" disabled="">
            <span id="printerName"></span> - <span id="fileName"></span>
          </button></h3>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span class="text-white" aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="container-fluid">
          <div class="row text-center">
            <div class="col-lg-12"><h5 class="mb-1 text-left">Date and Time</h5></div>
            <div class="col-lg-1"><button id="printStatus" type="button" class="btn btn-secondary float-right d-block" href="#" aria-expanded="false" disabled="">

              </button></div>
            <div class="col-lg-4"><button id="startDate" type="button" class="btn btn-secondary float-right d-block" href="#" aria-expanded="false" disabled="">

              </button></div>
            <div class="col-lg-3"><button id="printTime" type="button" class="btn btn-secondary float-right d-block" href="#" aria-expanded="false" disabled="">

              </button></div>
            <div class="col-lg-4"><button id="endDate" type="button" class="btn btn-secondary float-right d-block" href="#" aria-expanded="false" disabled="">

              </button></div>
          </div>
          <div class="row">
            <div class="col-lg-6">
             <h5 class="mb-1 text-left">Filament</h5>
              <div class="input-group mb-3">
                <div class="input-group-prepend">
                  <label class="input-group-text" for="filament">Spool</label>
                </div>
                <select id="filament" class="custom-select">

                </select>
              </div>
                <div class="input-group mb-1 input-group-sm">
                  <div class="input-group-prepend">
                    <span class="input-group-text">Volume</span>
                  </div>
                  <input id="volume" type="text" class="form-control" placeholder="" aria-label="Recipient's username" aria-describedby="basic-addon2" disabled>
                  <div class="input-group-append">
                    <span class="input-group-text" id="basic-addon2">cm³</span>
                  </div>
                </div>
                <div class="input-group mb-1 input-group-sm">
                  <div class="input-group-prepend">
                    <span class="input-group-text">Length</span>
                  </div>
                  <input id="length" type="text" class="form-control" placeholder="" aria-label="Recipient's username" aria-describedby="basic-addon2" disabled>
                  <div class="input-group-append">
                    <span class="input-group-text" id="basic-addon2">m</span>
                  </div>
                </div>
              <form>
                <div class="row">
                  <div class="col p-0">
                    <div class="input-group mb-1 input-group-sm">
                      <div class="input-group-prepend">
                        <span class="input-group-text">Cost</span>
                      </div>
                      <input id="cost" type="text" class="form-control" placeholder="" aria-label="Recipient's username" aria-describedby="basic-addon2" disabled>
                      <div class="input-group-append">
                        <span class="input-group-text" id="basic-addon2"></span>
                      </div>
                    </div>
                  </div>
                  <div class="col p-0 pl-1">
                    <div class="input-group mb-1 input-group-sm">
                      <div class="input-group-prepend">
                        <span class="input-group-text">Weight</span>
                      </div>
                      <input id="weight" type="text" class="form-control" placeholder="" aria-label="Recipient's username" aria-describedby="basic-addon2" disabled>
                      <div class="input-group-append">
                        <span class="input-group-text" id="basic-addon2">g</span>
                      </div>
                    </div>
                  </div>
                </div>
              </form>
            </div>
            <div class="col-lg-6">
              <h5>Notes:</h5>
              <textarea id="notes" class="form-control" aria-label="With textarea" style="margin-top: 0px; margin-bottom: 0px; height: 142px;"></textarea>
            </div>
          </div>
          <div class="row">
            <div class="col-lg-6">
              <h5>File Information</h5>
              <div class="input-group mb-1 input-group-sm">
                <div class="input-group-prepend">
                  <span class="input-group-text">Upload Date</span>
                </div>
                <input id="dateUploaded" type="text" class="form-control" placeholder="" aria-label="Recipient's username" aria-describedby="basic-addon2" disabled>
              </div>
              <div class="input-group mb-1 input-group-sm">
                <div class="input-group-prepend">
                  <span class="input-group-text">Path</span>
                </div>
                <input id="path" type="text" class="form-control" placeholder="" aria-label="Recipient's username" aria-describedby="basic-addon2" disabled>
              </div>
              <div class="input-group mb-1 input-group-sm">
                <div class="input-group-prepend">
                  <span class="input-group-text">Size</span>
                </div>
                <input id="size" type="text" class="form-control" placeholder="" aria-label="Recipient's username" aria-describedby="basic-addon2" disabled>
                <div class="input-group-append">
                  <span class="input-group-text" id="basic-addon2">MB</span>
                </div>
              </div>
            </div>
            <div class="col-lg-6">
              <h5>File Statistics</h5>
              <div class="input-group mb-1 input-group-sm">
                <div class="input-group-prepend">
                  <span class="input-group-text">Estimated Print Time</span>
                </div>
                <input id="estimatedPrintTime" type="text" class="form-control" placeholder="" aria-label="Recipient's username" aria-describedby="basic-addon2" disabled>
              </div>
              <div class="input-group mb-1 input-group-sm">
                <div class="input-group-prepend">
                  <span class="input-group-text">Average Print Time</span>
                </div>
                <input id="averagePrintTime" type="text" class="form-control" placeholder="" aria-label="Recipient's username" aria-describedby="basic-addon2" disabled>
              </div>
              <div class="input-group mb-1 input-group-sm">
                <div class="input-group-prepend">
                  <span class="input-group-text">Last Print Time</span>
                </div>
                <input id="lastPrintTime" type="text" class="form-control" placeholder="" aria-label="Recipient's username" aria-describedby="basic-addon2" disabled>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer" id="historySave">
        <button type="button" class="btn btn-light" data-dismiss="modal">
          Close
        </button>
      </div>
    </div>
  </div>
</div>
<script src="/js/lib/modules/history.js" type="module"></script>

